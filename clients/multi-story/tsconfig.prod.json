{
    "compilerOptions": {
        "outDir": "dist",
        "target": "ES2020",
        "module": "ES2022",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "types": ["types-base", "@mult/types", "@types/jest"],
        "jsx": "react-jsx"
    }
}
